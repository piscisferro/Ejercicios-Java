/**
 * Escribe un programa que muestre tu horario de clase. Puedes usar espacios o tabuladores
 * para alinear el texto.
 *
 * @author Juan José Fernández Romero
 */

public class Ejercicio4 { // Clase principal
  public static void main(String[] args) {
    System.out.println("\tLunes" + "\tMartes" + "\tMiércoles" + "\tJueves" + "\tViernes");
    System.out.println("8:15" + "\tProg" + "\tProg" + "\tProg" + "\tSIM" + "\tBB.DD");
    System.out.println("9:15" + "\tProg" + "\tProg" + "\tProg" + "\tSIM" + "\tBB.DD");
    System.out.println("10:15" + "\tProg" + "\tLM" + "\tProg" + "\tSIM" + "\t\BB.DD");
    System.out.println("11:15" + "\tRecreo" + "\tRecreo" + "\tRecreo" + "\tRecreo" + "\tRecreo");
    System.out.println("11:45" + "\tFol" + "\tED" + "\tBB.DD" +"\tLM" + "\tSIM"); 
    System.out.println("12:45" + "\tFol" + "\tED" + "\tBB.DD" +"\tLM" + "\tSIM");
    System.out.println("13:45" + "\tFol" + "\tED" + "\tBB.DD" +"\tLM" + "\tSIM");
  }
}


